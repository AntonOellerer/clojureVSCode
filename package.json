{
    "name": "clojure",
    "displayName": "Clojure",
    "description": "Clojure nREPL support for Visual Studio Code",
    "version": "0.5.2",
    "publisher": "avli",
    "author": {
        "name": "Andrey Lisin",
        "email": "andrey.lisin@gmail.com"
    },
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages",
        "Other"
    ],
    "activationEvents": [
        "onLanguage:clojure"
    ],
    "main": "./out/src/clojureMain",
    "contributes": {
        "commands": [
            {
                "command": "clojureVSCode.eval",
                "title": "Clojure: Eval"
            },
            {
                "command": "clojureVSCode.evalAndShowResult",
                "title": "Clojure: Eval and show the result"
            },
            {
                "command": "clojureVSCode.connect",
                "title": "Clojure: Connect to nREPL"
            },
            {
                "command": "clojureVSCode.startNRepl",
                "title": "Clojure: Start & Connect to nREPL"
            },
            {
                "command": "clojureVSCode.stopNRepl",
                "title": "Clojure: Stop nREPL"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/jszip": "^0.0.31",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "bencoder": "^0.0.5",
        "jszip": "^3.1.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/avli/clojureVSCode"
    }
}
